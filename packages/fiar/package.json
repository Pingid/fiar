    "react-dom": "^18.2.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@firebase/app": "^0.9.29",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "typescript": "^5.4.2"
  },
  "peerDependencies": {
    "@firebase/app": "^0.9.27"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.cjs"
    },
    "./style.css": {
      "import": "./style.css",
      "require": "./style.css"
